{
    "vault": {
      "address": "<%= env('VAULT_ADDR') %>",
      "auth": {
        "type": "token",
        "config": {
            "token": "<%= env('VAULT_TOKEN') %>"
       }
      }
    },
    "secrets": [
      {
        "path": "secret/data",
        "format": "<%= key %>",
        "upcase": true
      }
    ]
}